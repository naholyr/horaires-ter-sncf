<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AboutTitle" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">19dip</item>
        <item name="android:textColor">@android:color/primary_text_dark</item>
    </style>
    <style name="AboutDivider">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2dip</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:src">@android:drawable/divider_horizontal_dark</item>
    </style>
</resources>